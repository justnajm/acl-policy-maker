{
    "name": "najm/acl-policy-maker",
    "description": "Using artisan generate ACL roles, permissions and laravel policy based on provided model name",
    "keywords": ["artisan", "laravel", "ACL", "Policy", "model","roles","permissions"],
    "license": "MIT",
    "authors": [
        {
            "name": "Najm Abideen",
            "email": "contact@najm.8008le.com",
            "homepage": "https://najm.8008le.com"
        }
    ],
    "require": {
        "php": "^7.1 || ^8.0",
        "laravel/framework": "^5.4 || ^6.0 || ^7.0 || ^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5 || ^7.0 || ^8.0",
        "graham-campbell/testbench": "^4.0 || ^5.0",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Najm\\AclPolicyMaker\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Najm\\AclPolicyMaker\\ServiceProvider"
            ]
        }
    }
}